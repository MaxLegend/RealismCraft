{
    "multipart": [
        {   "when": { "default": "true"},
            "apply": { "model": "realism:tree/leaves_spruce/default" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "realism:tree/leaves_spruce/down_connect", "uvlock": true }
        },
     	{   "when": { "up": "true"  },
            "apply": { "model": "realism:tree/leaves_spruce/up_connect", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "realism:tree/leaves_spruce/north_connect", "uvlock": true }
        },
        {   "when": { "east": "true"  },
            "apply": { "model": "realism:tree/leaves_spruce/east_connect", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "realism:tree/leaves_spruce/south_connect","uvlock": true }
        },
        {   "when": { "west": "true"  },
            "apply": { "model": "realism:tree/leaves_spruce/west_connect", "uvlock": true }
        }
    ]
}
