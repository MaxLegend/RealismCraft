{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "realism:tree/branch_birch/default" }
        },
   
     	{   "when": { "down_br": "true" },
            "apply": { "model": "realism:tree/branch_birch/down_branch", "uvlock": true }
        },
     	{   "when": { "up_br": "true"  },
            "apply": { "model": "realism:tree/branch_birch/up_branch", "uvlock": true }
        },
        {   "when": { "north_br": "true" },
            "apply": { "model": "realism:tree/branch_birch/north_branch", "uvlock": true }
        },
        {   "when": { "east_br": "true"  },
            "apply": { "model": "realism:tree/branch_birch/east_branch", "uvlock": true }
        },
        {   "when": { "south_br": "true" },
            "apply": { "model": "realism:tree/branch_birch/south_branch","uvlock": true }
        },
        {   "when": { "west_br": "true"  },
            "apply": { "model": "realism:tree/branch_birch/west_branch", "uvlock": true }
        }
    ]
}
