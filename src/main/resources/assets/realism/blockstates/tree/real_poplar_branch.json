{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "realism:tree/branch_poplar/default" }
        },
   
     	{   "when": { "down_br": "true" },
            "apply": { "model": "realism:tree/branch_poplar/down_branch", "uvlock": true }
        },
     	{   "when": { "up_br": "true"  },
            "apply": { "model": "realism:tree/branch_poplar/up_branch", "uvlock": true }
        },
        {   "when": { "north_br": "true" },
            "apply": { "model": "realism:tree/branch_poplar/north_branch", "uvlock": true }
        },
        {   "when": { "east_br": "true"  },
            "apply": { "model": "realism:tree/branch_poplar/east_branch", "uvlock": true }
        },
        {   "when": { "south_br": "true" },
            "apply": { "model": "realism:tree/branch_poplar/south_branch","uvlock": true }
        },
        {   "when": { "west_br": "true"  },
            "apply": { "model": "realism:tree/branch_poplar/west_branch", "uvlock": true }
        }
    ]
}
