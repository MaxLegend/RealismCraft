{
    "multipart": [
        {   "when": { "default": "true"},
            "apply": { "model": "realism:tree/leaves_oak/default" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "realism:tree/leaves_oak/down_connect", "uvlock": true }
        },
     	{   "when": { "up": "true"  },
            "apply": { "model": "realism:tree/leaves_oak/up_connect", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "realism:tree/leaves_oak/north_connect", "uvlock": true }
        },
        {   "when": { "east": "true"  },
            "apply": { "model": "realism:tree/leaves_oak/east_connect", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "realism:tree/leaves_oak/south_connect","uvlock": true }
        },
        {   "when": { "west": "true"  },
            "apply": { "model": "realism:tree/leaves_oak/west_connect", "uvlock": true }
        }
    ]
}
