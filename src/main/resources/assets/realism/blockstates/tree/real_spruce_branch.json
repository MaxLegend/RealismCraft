{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "realism:tree/branch_spruce/default" }
        },
     
     	{   "when": { "down_br": "true" },
            "apply": { "model": "realism:tree/branch_spruce/down_branch", "uvlock": true }
        },
     	{   "when": { "up_br": "true"  },
            "apply": { "model": "realism:tree/branch_spruce/up_branch", "uvlock": true }
        },
        {   "when": { "north_br": "true" },
            "apply": { "model": "realism:tree/branch_spruce/north_branch", "uvlock": true }
        },
        {   "when": { "east_br": "true"  },
            "apply": { "model": "realism:tree/branch_spruce/east_branch", "uvlock": true }
        },
        {   "when": { "south_br": "true" },
            "apply": { "model": "realism:tree/branch_spruce/south_branch","uvlock": true }
        },
        {   "when": { "west_br": "true"  },
            "apply": { "model": "realism:tree/branch_spruce/west_branch", "uvlock": true }
        }
    ]
}
